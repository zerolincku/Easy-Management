<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linck.management.quartz.mapper.SysJobLogMapper">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , job_id ,msg, result, spend_time, create_time
    </sql>

    <select id="list" resultType="com.linck.management.quartz.model.entity.SysJobLog"
            parameterType="com.linck.management.quartz.model.dto.SysJobLogDto">
        SELECT
        <include refid="Base_Column_List"></include>
        FROM
        sys_job_log
        <where>
            <if test="id != null">
                AND id = #{id}
            </if>
            <if test="jobId != null">
                AND job_id = #{jobId}
            </if>
            <if test="msg != null and msg != ''">
                AND msg like #{msg}
            </if>
            <if test="result != null and result != ''">
                AND result = #{result}
            </if>
        </where>
    </select>
</mapper>
