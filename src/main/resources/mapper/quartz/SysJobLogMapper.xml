<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linck.management.quartz.mapper.SysJobLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.linck.management.quartz.model.entity.SysJobLog">
        <id column="id" property="id"/>
        <result column="msg" property="msg"/>
        <result column="result" property="result"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , msg, result, create_time, update_time
    </sql>

    <select id="list" resultMap="BaseResultMap" parameterType="com.linck.management.quartz.model.dto.SysJobLogDTO">
        SELECT
        <include refid="Base_Column_List"></include>
        FROM
        sys_job
        <where>
            <if test="id != null">
                AND id = #{id}
            </if>
            <if test="jobId != null">
                AND job_id = #{jobId}
            </if>
            <if test="msg != null and msg != ''">
                AND msg like #{msg}
            </if>
            <if test="result != null and result != ''">
                AND result = #{result}
            </if>
        </where>
    </select>
</mapper>
